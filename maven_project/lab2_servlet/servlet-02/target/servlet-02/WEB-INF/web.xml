<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                         http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="true">
  <context-param>
    <param-name>url</param-name>
    <param-value>jdbc:mysql://localhost:3306/mybatis</param-value>
  </context-param>
  <servlet>
    <servlet-name>hello</servlet-name>
    <servlet-class>com.eric.servlet.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>hello</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getname</servlet-name>
    <servlet-class>com.eric.servlet.GetServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getname</servlet-name>
    <url-pattern>/getname</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>geturl</servlet-name>
    <servlet-class>com.eric.servlet.ServletGetPara</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>geturl</servlet-name>
    <url-pattern>/geturl</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getdispatch</servlet-name>
    <servlet-class>com.eric.servlet.ServletDispatch</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getdispatch</servlet-name>
    <url-pattern>/getdispatch</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getprop</servlet-name>
    <servlet-class>com.eric.servlet.ServletProp</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getprop</servlet-name>
    <url-pattern>/getprop</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getdownload</servlet-name>
    <servlet-class>com.eric.servlet.ServletDownload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getdownload</servlet-name>
    <url-pattern>/getdownload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getdirec</servlet-name>
    <servlet-class>com.eric.servlet.ServletRedir</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getdirec</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getcookie</servlet-name>
    <servlet-class>com.eric.servlet.ServletCookie</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getcookie</servlet-name>
    <url-pattern>/cookie</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getsession</servlet-name>
    <servlet-class>com.eric.servlet.ServletSession</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getsession</servlet-name>
    <url-pattern>/session</url-pattern>
  </servlet-mapping>
</web-app>
